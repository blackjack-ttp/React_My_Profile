{
  "compilerOptions": {
    "baseUrl": "/",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@Image/*": [
        "src/assets/*"
      ],
      "@Imports/*": [
        "src/imports/*"
      ],
      "@Containers/*": [
        "src/containers/*"
      ],
      "@Components/*": [
        "src/components/*"
      ],
      "@Utils/*": [
        "src/utils/*"
      ],
      "@Styles/*": [
        "src/styles/*"
      ]
    }
  }
}
